{
  "name": "kubrick-code-npm-test",
  "version": "0.0.2",
  "type": "module",
  "main": "dist/index.js",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.12.7",
    "esbuild": "^0.20.2",
    "typescript": "^5.4.5"
  },
  "scripts": {
    "prepack": "yarn build",
    "build": "yarn clean && yarn build:tsc && yarn build:js",
